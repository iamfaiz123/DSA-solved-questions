// this is only a function 

void merge(int arr1[], int arr2[], int n, int m) {
	    int swap  = 0;
	    {
	        int k = 0;
	        for(auto  i = n -1 ; i > -1 ; i--)
	        {    if ( k == m)break;
	            else if(arr1[i] > arr2[k]){ swap = arr1[i]; arr1[i] = arr2[k] ; arr2[k] =swap;k++;}
	            
	            else break;
	        }
	        sort(arr1,arr1+n);
	        sort(arr2,arr2+m);
	        
	        
	    }
	    
	}
